# X-run script
rm /tmp/.X0-lock
Xvfb :0 -screen 0 1920x1080x24+32 -ac &
export DISPLAY=:0&&startxfce4 &
sleep 10
x0vncserver -display :0 -passwordfile ~/.vnc/passwd &
/usr/share/novnc/utils/launch.sh --vnc localhost:5900 &
export DISPLAY=:0&&xterm -e "xset s off"&&
#
# qps client connector
qps-client -p 8443 &
#
# start apps
export DISPLAY=:0 && ./thereminq-tensors/runfiles/rsaConverter &
#
# start rsaConverter, firefox session with lower yield rsa keygen site 
# and shors demo on generated 1kbit rsa key ( which will eat the host ) 
# 
sleep 10 && export DISPLAY=:0 && firefox https://www.mobilefish.com/services/rsa_key_generation/rsa_key_generation.php &
export DISPLAY=:0 && xterm -e "/root/shors_modulus.sh > /var/log/thereminq/shors-modulus_'$(date +%F_%R_%S).log'" 
#
echo "done"
# end run, do not stop container
tail -f /dev/null
