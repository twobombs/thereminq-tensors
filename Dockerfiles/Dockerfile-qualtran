FROM twobombs/thereminq-tensors

RUN git clone https://github.com/quantumlib/Qualtran.git && cd Qualtran/ && pip install -e . && python -m qualtran.surface_code.ui

RUN apt update && apt install postgresql && apt clean all
RUN https://github.com/ioanamoflic/pandora.git

EXPOSE 6080
ENTRYPOINT /root/run-nv
